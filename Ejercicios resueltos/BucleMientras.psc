Algoritmo BucleMientras
	
	//Mientras <condicion> Hacer
		//condicion <-condicion + 1
	//
	Definir num Como Entero
	num <- 2
	Mientras i < 10 Hacer
		Escribir num, "x", i, " = ", num * i
		i <- i + 1 //De uno en uno
	FinMientras
	
	Para i<-1 Hasta 100 Hacer
		Si i MOD 2 == 0
			Escribir i
		FinSi		
	FinPara
	
	
FinAlgoritmo
